# Development environment variables for Backend
# Copy this file to .env and fill with your values

NODE_ENV=development
PORT=3001

# API Configuration
API_BASE_URL=https://brasilapi.com.br/api/v1
CORS_ORIGIN=http://localhost:5173

# Logging
LOG_LEVEL=debug

# Cache Configuration (1 hour for development)
CACHE_TTL=3600

# Rate Limiting (more permissive for development)
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=1000

# Health Check
HEALTH_CHECK_INTERVAL=30000
